{
  "name": "alerts",
  "description": "Alerts",
  "sub_commands": [
    {
      "name": "show",
      "description": "Displays Alerts On the cli",
      "need_confirmation": false,
      "permissions_tag": "list",
      "args": [
        {
          "flag": "args",
          "default": [],
          "suppress_help": true,
          "nargs": "?",
          "help": ""
        },
        {
          "flag": "-f",
          "dest": "format",
          "help": "Format of Output",
          "default": "table",
          "type": "str",
          "choices": [
            "table",
            "xml",
            "json"
          ]
        }
      ],
      "comm": {
        "type": "rest",
        "method": "get",
        "target": "/halo/alerts",
        "version": "v2"
      },
      "output": {
        "table": {
          "headers": {
            "alert_uuid": "Alert Id",
            "description": "Description",
            "severity": "Severity",
            "state": "State",
            "acknowledged": "Acknowledged",
            "resolved": "Resolved",
            "hostname": "Hostname"
          },
          "filters": "alerts"
        }
      }
    }
  ]
}