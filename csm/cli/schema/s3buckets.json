{
  "name": "s3buckets",
  "description": "Bucket",
  "sub_commands": [
    {
      "name": "show",
      "description": "Displays S3 buckets On The CLI",
      "need_confirmation": false,
      "permissions_tag": "list",
      "args": [
        {
          "flag": "-f",
          "dest": "format",
          "help": "Format of Output",
          "default": "table",
          "type": "str",
          "choices": [
            "table",
            "xml",
            "json"
          ]
        },
        {
          "flag": "args",
          "default": [],
          "suppress_help": true,
          "nargs": "?"
        }
      ],
      "comm": {
        "type": "rest",
        "method": "get",
        "target": "/{version}/buckets",
        "version": "v2"
      },
      "output": {
        "table": {
          "headers": {
            "id": "Bucket id"
          },
          "filters": "buckets"
        }
      }
    },
    {
      "name": "create",
      "description": "Create new S3 bucket",
      "need_confirmation": false,
      "permissions_tag": "create",
      "args": [
        {
          "flag": "bucket_name",
          "type": "str",
          "help": "Give a bucket name to create.",
          "json": true
        },
        {
          "flag": "location",
          "type": "str",
          "help": "Location of the bucket.",
          "nargs": "?",
          "json": true
        },
        {
          "flag": "object_lock",
          "type": "bool",
          "help": "Object lock on bucket.",
          "nargs": "?",
          "json": true
        },
        {
          "flag": "args",
          "default": [],
          "suppress_help": true,
          "nargs": "?"
        }
      ],
      "comm": {
        "type": "rest",
        "method": "post",
        "target": "/{version}/buckets",
        "version": "v2"
      },
      "output": {
        "success": "Bucket created."
      }
    }
  ]
}
 